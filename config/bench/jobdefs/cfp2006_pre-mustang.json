{
  "job_name": "cfp2006_pre-mustang",
  "timeout": 691200,
  "actions":
  [
    {
      "command": "deploy_linaro_image",
      "parameters":
      {
        "role": "host",
        "image": "http://images.validation.linaro.org/ubuntu-14-04-server-base.img.gz"
      }
    },
    {
      "command": "deploy_linaro_kernel",
      "parameters":
      {
        "role": "target",
        "dtb": "http://people.linaro.org/~bernie.ogden/apm-mustang.dtb",
        "kernel": "http://people.linaro.org/~bernie.ogden/linux-4.2.3-mustang/arch/arm64/boot/uImage",
        "nfsrootfs": "http://people.linaro.org/~bernie.ogden/linaro-utopic-developer-20150114-87.tar.gz"
      }
    },
    {
      "command": "lava_test_shell",
      "parameters":
      {
        "role": "host",
        "timeout": 691200,
        "testdef_repos":
        [{
          "git-repo": "https://git.linaro.org/toolchain/abe",
          "testdef": "config/bench/lava/host-session.yaml",
          "revision": "benchmarking",
          "parameters":
          {
            "BENCHMARK": "CPU2006",
            "TOOLCHAIN": "https://releases.linaro.org/15.02/components/toolchain/binaries/arm-linux-gnueabihf/gcc-linaro-4.9-2015.02-3-x86_64_arm-linux-gnueabihf.tar.xz",
            "RUN_FLAGS": "fp",
            "COMPILER_FLAGS": "",
            "MAKE_FLAGS": "BENCHSET=fp",
            "PREBUILT": "<USER@HOST>:<PATH TO CPU2006.git.tar.xz>",
            "BENCH_DEBUG": "1",
            "PUB_KEY": "<YOUR PUBLIC KEY>"
          }
        }]
      }
    },
    {
      "command": "lava_test_shell",
      "parameters":
      {
        "role": "target",
        "timeout": 691200,
        "testdef_repos":
        [{
          "git-repo": "https://git.linaro.org/toolchain/abe",
          "testdef": "config/bench/lava/target-session-mustang.yaml",
          "revision": "benchmarking",
          "parameters":
          {
            "CONFIG": "mustang",
            "PUB_KEY": "<YOUR PUBLIC KEY>"
          }
        }]
      }
    },
    {
      "command": "submit_results",
      "parameters":
      {
        "server": "https://<YOUR LAVA USER NAME>@validation.linaro.org/RPC2/",
        "stream": "/private/personal/<YOUR LAVA USER NAME>/"
      }
    }
  ],
  "device_group":
  [
    {
      "count": 1,
      "device_type": "kvm",
      "tags": [ "tcwg" ],
      "role": "host"
    },
    {
      "count": 1,
      "device_type": "mustang",
      "role": "target"
    }
  ]
}

