{
  "job_name": "cint2006-kvm_x1",
  "timeout": 691200,
  "actions":
  [
    {
      "command": "deploy_linaro_image",
      "parameters":
      {
        "role": "host",
        "image": "http://images.validation.linaro.org/ubuntu-14-04-server-base.img.gz"
      }
    },
    {
      "command": "deploy_linaro_image",
      "parameters":
      {
        "role": "target",
        "image": "http://images.validation.linaro.org/ubuntu-14-04-server-base.img.gz"
      }
    },
    {
      "command": "lava_test_shell",
      "parameters":
      {
        "role": "host",
        "timeout": 691200,
        "testdef_repos":
        [{
          "git-repo": "https://git.linaro.org/toolchain/abe",
          "testdef": "config/bench/lava/host-session.yaml",
          "revision": "benchmarking",
          "parameters":
          {
            "BENCHMARK": "CPU2006",
            "TOOLCHAIN": "/usr/bin/gcc",
            "RUN_FLAGS": "--size test -n 1 --tune base int",
            "COMPILER_FLAGS": "",
            "MAKE_FLAGS": "BENCHSET=int",
            "PREBUILT": "",
            "BENCH_DEBUG": "1",
            "PUB_KEY": "<YOUR PUBLIC KEY>"
          }
        }]
      }
    },
    {
      "command": "lava_test_shell",
      "parameters":
      {
        "role": "target",
        "timeout": 691200,
        "testdef_repos":
        [{
          "git-repo": "https://git.linaro.org/toolchain/abe",
          "testdef": "config/bench/lava/target-session-kvm.yaml",
          "revision": "benchmarking",
          "parameters":
          {
            "CONFIG": "kvm",
            "PUB_KEY": "<YOUR PUBLIC KEY>"
          }
        }]
      }
    },
    {
      "command": "submit_results",
      "parameters":
      {
        "server": "https://<YOUR LAVA USER NAME>@validation.linaro.org/RPC2/",
        "stream": "/private/personal/<YOUR LAVA USER NAME>/"
      }
    }
  ],
  "device_group":
  [
    {
      "count": 1,
      "device_type": "kvm",
      "tags": [ "tcwg" ],
      "role": "host"
    },
    {
      "count": 1,
      "device_type": "kvm",
      "tags": [ "tcwg" ],
      "role": "target"
    }
  ]
}

