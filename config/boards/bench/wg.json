{
    "job_name": "bench-wg",
    "device_type": "wg",
    "timeout": 1800,
    "actions": [
        {
            "command": "deploy_linaro_image",
            "parameters": {
                "hwpack": "http://people.linaro.org/~fathi.boudra/juno-firmware-0.8.1/hwpack_linaro-lt-vexpress64-rtsm_20140823-686_arm64_supported.tar.gz",
                "rootfs": "http://snapshots.linaro.org/openembedded/images/minimal-armv8-gcc-4.9/711/linaro-image-minimal-genericarmv8-20140828-711.rootfs.tar.gz"
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/toolchain/lavabench.git",
                        "testdef": "bench-session-oe.yaml",
                        "parameters": {
                           "GATEWAY": "10.0.0.1",
                           "PUB_KEY": "",
                           "LISTENER_ADDR": "",
                           "LISTENER_PORT": ""
                        }
                    }
                ],
                "timeout": 691200
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "",
                "stream": ""
            }
        }
    ]
}

